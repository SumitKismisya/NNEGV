//NNEGV


#include<stdio.h>
#include<math.h>
void main()
{
int i,n,count=0;
clrscr();
printf("\nUpto which number:");
scanf("%d",&n);
for(i=1; i<=n; i++)
{
      if(n%i==0)
      {
           count++;
           }
           }
           if(count==2)
           {
                 printf("\nEntered number is prime");
                 }
                 else
                 {
                 printf("\nEntered number is not prime");
                 }
                 getch();
}
